super + F1
    $BROWSER https://www.icloud.com/find/

super + F2
    redshift -P -O 5000

super + F3
    redshift -P -O 3500

super + r
    $BROWSER

super + c
    $BROWSER --incognito

super + Return
    $TERMINAL

super + z
    passmenu -fn "Hack Nerd Font"

super + q
    sortie.sh

super + BackSpace
    wmctrl -c :ACTIVE:

super + y
    rofi -show drun -show-icons

super + shift + s
    pkill -USR1 -x sxhkd; notify-send 'sxhkd' 'Reloaded config'

Print
    screenshooter.sh select

ctrl + Print
    screenshooter.sh window

